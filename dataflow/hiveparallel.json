{
	"name": "hiveparallel",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "skshadoopls",
						"type": "LinkedServiceReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          fyear as string,",
				"          fmonth as string,",
				"          fdayofmonth as string,",
				"          fdayofweek as string,",
				"          deptime as integer,",
				"          arrtime as integer,",
				"          uniquecarrier as string,",
				"          origin as string,",
				"          dest as string,",
				"          distance as integer,",
				"          isdepdelayed as string,",
				"          isdepdelayed_rec as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     limit: 100,",
				"     format: 'table',",
				"     store: 'hive',",
				"     schemaName: 'default',",
				"     tableName: 'airlinestest',",
				"     staged: true,",
				"     storageContainer: 'AzureBlob1',",
				"     storageFolderPath: '',",
				"     stagingDatabaseName: 'adftemp') ~> source1"
			]
		}
	}
}