{
	"name": "dataflow3",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "AzureSynapseAnalyticsTable1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText1",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          EmployeeKey as integer,",
				"          ParentEmployeeKey as integer,",
				"          EmployeeNationalIDAlternateKey as string,",
				"          ParentEmployeeNationalIDAlternateKey as string,",
				"          SalesTerritoryKey as integer,",
				"          FirstName as string,",
				"          LastName as string,",
				"          MiddleName as string,",
				"          NameStyle as boolean,",
				"          Title as string,",
				"          HireDate as date,",
				"          BirthDate as date,",
				"          LoginID as string,",
				"          EmailAddress as string,",
				"          Phone as string,",
				"          MaritalStatus as string,",
				"          EmergencyContactName as string,",
				"          EmergencyContactPhone as string,",
				"          SalariedFlag as boolean,",
				"          Gender as string,",
				"          PayFrequency as integer,",
				"          BaseRate as decimal(19,4),",
				"          VacationHours as integer,",
				"          SickLeaveHours as integer,",
				"          CurrentFlag as boolean,",
				"          SalesPersonFlag as boolean,",
				"          DepartmentName as string,",
				"          StartDate as date,",
				"          EndDate as date,",
				"          Status as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     enableCdc: true,",
				"     mode: 'read',",
				"     waterMarkColumn: 'EndDate',",
				"     batchSize: 30,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table',",
				"     staged: true) ~> source1",
				"source1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     format: 'table') ~> sink1"
			]
		}
	}
}